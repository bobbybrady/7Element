@model IEnumerable<_7Element.Models.DonatedTickets>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PredsGame)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailAddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailTitle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailBody)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TransactionComplete)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.User.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PredsGame.PredsGameId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailTitle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailBody)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionComplete)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DonatedTicketsId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DonatedTicketsId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DonatedTicketsId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
